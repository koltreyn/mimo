    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3823"/>
    <skos:prefLabel xml:lang="en">423.213-71 Labrosones with fingerholes and with keys, with (wider) conical bore</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3819"/>
