  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/2">
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/3"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:prefLabel xml:lang="en">11 Struck idiophones</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6686"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3100"/>
    <skos:definition xml:lang="en">The substance of the instrument itself, owing to its solidity and elasticity, vibrates and may radiate sound without requiring stretched membranes or strings. The instrument is made to vibrate by being struck.</skos:definition>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/1"/>
