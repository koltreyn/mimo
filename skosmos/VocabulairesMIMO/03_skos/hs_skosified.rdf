  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/99">
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/6430"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/2894"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/4696"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:definition xml:lang="en">Joined sets of drums with single tightly stretched membranes enclosing drum bodies in the form of vessels that are curvilinear or rectilinear in profile. The membranes are directly struck by the player using the hands or intermediate devices such as sticks. West Asian permanently joined pairs of kettledrums.</skos:definition>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/2910"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5339"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/4680"/>
