    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/57"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/199">
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/200"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/202"/>
    <skos:prefLabel xml:lang="en">321.31 Spike lutes</skos:prefLabel>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/198"/>
    <skos:definition xml:lang="en">One or more strings are stretched between fixed points. The string bearer and the resonator are organically united and cannot be separated without destroying the instrument. The plane of the strings runs parallel with the sound-table. The string bearer is a plain handle that passes diametrically through or over the resonator.</skos:definition>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/201"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6846"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/149">
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:prefLabel xml:lang="en">311.121.1 Mono-heterochord musical bows without resonator</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3529"/>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/148"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">A single string is stretched between fixed points. The instrument consists of a string bearer, and it has no resonator (the human mouth is not to be taken into account as a resonator). The string bearer is flexible (and curved). The string is of separate material from the string bearer. NB: if a separate, unattached resonator is used, the specimen belongs to 311.121.21.</skos:definition>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/6451"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3523"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/150"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5355"/>
