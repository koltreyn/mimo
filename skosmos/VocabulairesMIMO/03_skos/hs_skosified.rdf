    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/112"/>
    <skos:prefLabel xml:lang="en">211.2 Tubular drums</skos:prefLabel>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/108"/>
