  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/6182">
    <skos:prefLabel xml:lang="en">533.1 Analogue modules</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/6184"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/6183"/>
    <skos:definition xml:lang="en">Continuously varying electrical signals are passed to a loudspeaker to produce sound. The electrical signals are generated using electronic circuitry. Modules containing analogue fully electronic devices used to produce, process and communicate electronic sound signals and/or sequences of signals.</skos:definition>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/6181"/>
