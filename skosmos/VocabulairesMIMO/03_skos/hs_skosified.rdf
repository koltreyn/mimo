    <skos:prefLabel xml:lang="en">541.11 Digital synthesizers using frequency modulation synthesis, without fixed keyboard controllers</skos:prefLabel>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/6189"/>
