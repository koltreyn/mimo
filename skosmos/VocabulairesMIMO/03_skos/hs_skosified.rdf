    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">315.1-5 Trough zithers without resonator sounded by the bare fingers</skos:prefLabel>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:definition xml:lang="en">Strings are stretched between fixed points across the mouth of a rigid trough. The strings are often formed from a single length threaded through several holes. The trough has no additional resonator. Sounded by the bare fingers.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/186">
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/187"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/188"/>
    <skos:prefLabel xml:lang="en">315 Trough zithers</skos:prefLabel>
