  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/6195">
    <skos:prefLabel xml:lang="en">544 Digital samplers and sampling synthesizers</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/6187"/>
    <skos:definition xml:lang="en">Electrical signals are generated in the form of quantized sequences of pulses. These are converted to continuous signals that activate a loudspeaker. The electrical signals are generated using electronically stored data and electronic circuitry. Configurations containing devices to digitally design and process electronic sound signals and/or sequences of signals. Digital samplers and sampling synthesizers.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/46">
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/8353"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/8351"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
