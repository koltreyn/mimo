  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/6170">
    <skos:prefLabel xml:lang="en">531.2 Analogue synthesizers and other electronic instruments with solid state circuitry (transistor and/or analogue integrated circuitry) generating and/or processing electric sound signals</skos:prefLabel>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/6168"/>
