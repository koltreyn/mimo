    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/8313"/>
    <skos:prefLabel xml:lang="en">211.311 Single-skin frame drums without handle</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/2508"/>
    <skos:definition xml:lang="en">Drums in which the depth of the body does not exceed the radius of the membrane. The body has no rigid handle. The single tightly stretched membrane is directly struck by the player, using the hands or intermediate devices such as sticks. Tambourine.</skos:definition>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/4857"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/2794"/>
