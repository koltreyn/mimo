    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5198"/>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/245"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/280"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/4101"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/248"/>
    <skos:definition xml:lang="en">The air itself is the vibrator in the primary sense. The standing waves are significantly confined within the instrument itself. A narrow stream of air is directed against an edge to excite a column of air in a tube or a body of air in a cavity.</skos:definition>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/4720"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:prefLabel xml:lang="en">421 Edge instruments or flutes</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/InstrumentsKeywords/2704">
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/107"/>
  </rdf:Description>
