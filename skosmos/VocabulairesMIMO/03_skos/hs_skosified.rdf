    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/4010"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/237">
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/229"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6320"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">412.14 Ribbon reeds</skos:prefLabel>
    <skos:definition xml:lang="en">Reed instruments that are free aerophones are sounded by a flow of air in which the reed is the primary vibrator. The vibrating air is not confined by the instrument. The air-stream is directed against the edge of a stretched band or ribbon. The acoustics of this process have not yet been studied. British Columbia.</skos:definition>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/155">
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/154"/>
    <skos:definition xml:lang="en">A single string is stretched between fixed points. The instrument consists of a string bearer with a resonator that can be detached without destroying the sound-producing apparatus. The string bearer is flexible (and curved). The string is of separate material from the string bearer. The string has no tuning noose. S. Africa (hade, thomo).</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">311.121.221 Mono-heterochord musical bows with resonator attached, without tuning noose</skos:prefLabel>
  </rdf:Description>
