    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/135"/>
    <skos:definition xml:lang="en">The sound is excited by tightly stretched membranes made to vibrate by friction.</skos:definition>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:prefLabel xml:lang="en">23 Friction drums</skos:prefLabel>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/124"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/130"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Description>
