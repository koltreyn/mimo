    <skos:prefLabel xml:lang="en">112.21 Scraped sticks</skos:prefLabel>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/51"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/4773"/>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/48"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3056"/>
    <skos:definition xml:lang="en">The substance of the instrument itself, owing to its solidity and elasticity, vibrates and radiates sound when scraped. A notched stick is scraped with a little stick.</skos:definition>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/50"/>
  </rdf:Description>
