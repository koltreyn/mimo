    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3037"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/5573">
    <skos:prefLabel xml:lang="en">211.272.1 Individual double-skin cylindro-conical drums</skos:prefLabel>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/5572"/>
