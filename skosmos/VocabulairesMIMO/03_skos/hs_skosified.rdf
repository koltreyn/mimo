    <skos:prefLabel xml:lang="en">314.122-6-8+421.222-62-8 Boxzither, sounded by plectrum, with keyboard + set of duct flutes, with flexible air reservoir, with keyboard</skos:prefLabel>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
