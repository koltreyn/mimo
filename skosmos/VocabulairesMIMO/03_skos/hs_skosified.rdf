  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/7">
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5317"/>
