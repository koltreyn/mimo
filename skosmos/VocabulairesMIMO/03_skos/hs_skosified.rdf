  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/173">
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/171"/>
    <skos:prefLabel xml:lang="en">312.22 Heterochord half-tube zithers</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3612"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3717"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3724"/>
    <skos:definition xml:lang="en">One or more strings are stretched between fixed points along the convex surface of a gutter. The strings are of separate material from the string bearer. E. Asia.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/InstrumentsKeywords/2480">
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/10"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/20">
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6308"/>
    <skos:definition xml:lang="en">An instrument in the form of a tube, owing to the solidity and elasticity of its substance, vibrates and radiates sound when struck either with non-sonorous objects (hand, stick, striker) or against non-sonorous objects (human body, the ground). The player creates clearly defined individual strokes. This group does not include slit drums, which are a sub-group of bells, 111.243.</skos:definition>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/19"/>
    <skos:prefLabel xml:lang="en">111.231 (Individual) percussion tubes</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5297"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5152"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6282"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/InstrumentsKeywords/3888">
