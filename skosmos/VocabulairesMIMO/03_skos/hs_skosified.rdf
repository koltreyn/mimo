  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/227">
    <skos:prefLabel xml:lang="en">411 Displacement free aerophones</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/8399"/>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/226"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:definition xml:lang="en">The air itself is the vibrator in the primary sense. The vibrating air is not confined by the instrument. The air-stream meets a sharp edge, or a sharp edge is moved through the air. In either case a periodic displacement of air occurs to alternate flanks of the edge. Whip, sword-blade.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/InstrumentsKeywords/2598">
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/114"/>
