  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/31">
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/30"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/2384"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Individual percussion vessels, owing to the solidity and elasticity of their substance, vibrate and radiate sound when struck with non-sonorous objects (hand, stick, striker). The vibration is weakest near the apex from which the instrument is suspended. The bell has no internal striker; the player creates clearly defined individual strokes on its external surface, using a separate beater.</skos:definition>
    <skos:prefLabel xml:lang="en">111.242.121 (Individual) suspended bells struck from the outside</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6796"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/2390"/>
  </rdf:Description>
