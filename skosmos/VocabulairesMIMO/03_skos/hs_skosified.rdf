  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/83">
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/87"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/1"/>
    <skos:definition xml:lang="en">The substance of the instrument itself, owing to its solidity and elasticity, vibrates and radiates sound. The instrument is made to vibrate by being blown upon.</skos:definition>
    <skos:prefLabel xml:lang="en">14 Blown idiophones</skos:prefLabel>
