  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/100">
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/111"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
