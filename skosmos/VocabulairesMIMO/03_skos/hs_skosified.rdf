    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6967"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5223"/>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/97"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/InstrumentsKeywords/4133">
