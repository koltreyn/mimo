  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/358">
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3822"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6334"/>
