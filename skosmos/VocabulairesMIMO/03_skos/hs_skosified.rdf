    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6927"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3360"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5269"/>
    <skos:prefLabel xml:lang="en">321.311 Spike bowl lutes</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3203"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6919"/>
