  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/2167">
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/2170"/>
    <skos:prefLabel xml:lang="en">423.232.1 Valve horns with narrow bore</skos:prefLabel>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/359"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
