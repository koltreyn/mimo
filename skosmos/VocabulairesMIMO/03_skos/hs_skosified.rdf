    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3385"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">321.33 Half-spike lutes or tanged lutes</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/125">
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/126"/>
    <skos:definition xml:lang="en">The sound is excited by a tightly stretched membrane made to vibrate by friction with a stick that passes through a hole in it. The stick is either itself rubbed, or is employed to rub the membrane.</skos:definition>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/124"/>
    <skos:prefLabel xml:lang="en">231.1 Friction drums with inserted stick</skos:prefLabel>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/127"/>
