  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/139">
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">The sound is excited by a tightly stretched membrane made to vibrate by speaking or singing into it; the sound of the membrane modifies that of the voice. The membrane is placed on top of a tube or box. Africa; while also, E. Asian flutes with a lateral hole sealed by a membrane, exhibit an affinity with the principle of the tube kazoo.</skos:definition>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/2351"/>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/137"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/2349"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/2352"/>
    <skos:prefLabel xml:lang="en">242 Tube- or vessel-kazoos</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/2353"/>
  </rdf:Description>
