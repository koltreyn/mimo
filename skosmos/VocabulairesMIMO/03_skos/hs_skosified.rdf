    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5320"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6803"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5514"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5302"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5455"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5373"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5237"/>
    <skos:definition xml:lang="en">One or more strings are stretched between fixed points. The instrument consists of a rigid string bearer, or of a rigid string bearer with a resonator which is not integral and can be detached without destroying the sound-producing apparatus. The string bearer is a complete tube. The strings are cut from the bark of the tube, remaining attached at each end. Africa and Indonesia (gonra, togo, valiha).</skos:definition>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3706"/>
  </rdf:Description>
