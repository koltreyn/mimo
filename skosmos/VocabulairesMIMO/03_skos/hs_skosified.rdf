  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/168">
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/5374"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:prefLabel xml:lang="en">312.12 Heterochord (true) tube zithers</skos:prefLabel>
    <skos:definition xml:lang="en">One or more strings are stretched between fixed points. The instrument consists of a rigid string bearer, or of a rigid string bearer with a resonator which is not integral and can be detached without destroying the sound-producing apparatus. The string bearer is a complete tube. The strings are of separate material from the tube.</skos:definition>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/166"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/169"/>
