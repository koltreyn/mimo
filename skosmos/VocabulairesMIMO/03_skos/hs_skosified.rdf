    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3729"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/57">
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/1"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/65"/>
    <skos:definition xml:lang="en">The substance of the instrument itself, owing to its solidity and elasticity, vibrates and radiates sound. Lamellae, i.e. elastic plaques, fixed at one end, are flexed and then released to return to their position of rest.</skos:definition>
    <skos:prefLabel xml:lang="en">12 Lamellaphones (or plucked idiophones)</skos:prefLabel>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/58"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:narrower rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/6420"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/4836"/>
  </rdf:Description>
