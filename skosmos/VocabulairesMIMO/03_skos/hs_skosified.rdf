    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3779"/>
    <skos:prefLabel xml:lang="en">422.112+422.121.2-71+422.22-62 Conical double-reed chanter + set of cylindrical double-reed regulator with stopped end with keys + set of cylindrical single-reed drones; flexible air reservoir</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
