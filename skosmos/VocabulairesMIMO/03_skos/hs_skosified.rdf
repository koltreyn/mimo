  </rdf:Description>
  <rdf:Description rdf:about="http://www.mimo-db.eu/HornbostelAndSachs/6460">
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3613"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3666"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3697"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3638"/>
    <skos:definition xml:lang="en">One or more strings are stretched between fixed points. The string bearer is a rigid board, with a box-shaped resonator made of slats which is not integral and can be detached without destroying the sound-producing apparatus. The plane of the strings is parallel with that of the string bearer. Sounded by plectrum.</skos:definition>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3615"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:prefLabel xml:lang="en">314.122-6 True board zithers with resonator box (box zither) sounded by plectrum</skos:prefLabel>
    <skos:broader rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs/182"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/3649"/>
