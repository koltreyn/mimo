    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6988"/>
    <skos:inScheme rdf:resource="http://www.mimo-db.eu/HornbostelAndSachs"/>
    <skos:exactMatch rdf:resource="http://www.mimo-db.eu/InstrumentsKeywords/6983"/>
  </rdf:Description>
